{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es2017",
		"declaration": false,
		"sourceMap": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strictNullChecks": true,
		"skipLibCheck": true
	},
	"exclude": [
		"../node_modules"
	],
	"include": [
		"../src"
	]
}
